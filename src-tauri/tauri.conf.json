{
  "productName": "template",
  "version": "0.1.0",
  "identifier": "com.template.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "template",
        "width": 1200,
        "height": 720,
        "minHeight": 480,
        "minWidth": 320,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "zoomHotkeysEnabled": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
