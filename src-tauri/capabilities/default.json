{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "shell:allow-open",
    "core:app:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:webview:allow-set-webview-zoom",
    "theme:default",
    "deep-link:default"
  ]
}
